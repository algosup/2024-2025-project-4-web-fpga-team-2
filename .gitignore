# Ignore installed dependencies  
node_modules/  

# Yarn error log file  
yarn-error.log  

# Next.js build folder  
.next/  

# Environment file (often contains secrets)  
.env  

# Ignore generated service worker and cache files  
**/public/precache.*.*.js  
**/public/sw.js  
**/public/workbox-*.js  
**/public/worker-*.js  
**/public/fallback-*.js  
**/public/precache.*.*.js.map  
**/public/sw.js.map  
**/public/workbox-*.js.map  
**/public/worker-*.js.map  
**/public/fallback-*.js  

# Ignore lock files in examples (avoids conflicts between package managers)  
examples/**/yarn.lock  
examples/**/package-lock.json  
examples/**/pnpm-lock.yaml  

# macOS and IDE-specific files  
.DS_STORE  
.idea  
*.iml  
.vscode  
*.sublime-project  
*.sublime-workspace  

# Ignore temporary and log files  
*~  
*.log*  
*.swp  
*.swo  

# Ignore generated files from testing and build tools  
__benchmarks__  
coverage/  
build/  
remote-repo/  
.module-cache  

# Ignore files related to Flow and Grunt  
scripts/flow/*/.flowconfig  
.flowconfig  
.grunt  

# Ignore files specific to React DevTools (prevents unnecessary builds and certificates)  
packages/react-devtools-core/dist  
packages/react-devtools-extensions/chrome/build  
packages/react-devtools-extensions/chrome/*.crx  
packages/react-devtools-extensions/chrome/*.pem  
packages/react-devtools-extensions/firefox/build  
packages/react-devtools-extensions/firefox/*.xpi  
packages/react-devtools-extensions/firefox/*.pem  
packages/react-devtools-extensions/shared/build  
packages/react-devtools-extensions/.tempUserDataDir  
packages/react-devtools-fusebox/dist  
packages/react-devtools-inline/dist  
packages/react-devtools-shell/dist  
packages/react-devtools-timeline/dist  

# Ignore circuit JSON files but keep config files
src/frontend/public/uploads/*.json
src/backend/uploads/*.json
src/database/circuits/*.json
*_circuit.json
*_netlist.json
# Then explicitly include important JSON files
!package.json
!package-lock.json
!tsconfig.json
!.eslintrc.json
!jest.config.json
